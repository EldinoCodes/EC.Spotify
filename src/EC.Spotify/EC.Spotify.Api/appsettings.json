{
  "AppSettings": {
    "ClientId": "clientId",
    "ClientSecret": "clientSecret",

    "AuthUri": "https://accounts.spotify.com/authorize",
    "TokenUri": "https://accounts.spotify.com/api/token",
    "RedirectUri": "redirect uri to this api instance (127.0.0.1 if localhost)",
    "Scopes": [
      "user-read-playback-state",
      "user-modify-playback-state"
    ],

    "UserUri": "https://api.spotify.com/v1/me",
    "SearchUri": "https://api.spotify.com/v1/search",
    "PlayerUri": "https://api.spotify.com/v1/me/player",
    "TrackUri": "https://api.spotify.com/v1/tracks"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
